{
  "name": "jg-dev",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "fix:prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,scss,json,md,mdx}\"",
    "lint": "concurrently npm:lint:*",
    "lint:next": "next lint",
    "lint:prettier": "prettier --check \"src/**/*.{js,jsx,ts,tsx,scss,json,md,mdx}\"",
    "lint:spellcheck": "spellchecker -f *.md ./**/*.md -d .spelling -l en-GB",
    "prepare": "husky install",
    "start": "next start",
    "test": "jest"
  },
  "dependencies": {
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@joshuagardiner/typescript-component-library": "^1.1.20",
    "@netlify/plugin-nextjs": "^4.2.5",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@types/node": "17.0.18",
    "@types/react": "17.0.39",
    "@types/react-dom": "^17.0.11",
    "concurrently": "^7.0.0",
    "eslint": "8.9.0",
    "eslint-config-next": "12.0.10",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "react-icons": "^4.3.1",
    "spellchecker-cli": "^4.8.1",
    "typescript": "4.5.5"
  }
}
