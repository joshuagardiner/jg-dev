{
  "name": "jg-dev",
  "private": true,
  "version": "1.2.1",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "fix:prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,scss,json,md,mdx}\"",
    "lint": "concurrently npm:lint:*",
    "lint:next": "next lint",
    "lint:prettier": "prettier --check \"src/**/*.{js,jsx,ts,tsx,scss,json,md,mdx}\"",
    "lint:spellcheck": "spellchecker -f *.md ./**/*.md -d .spelling -l en-GB",
    "prepare": "husky install",
    "start": "next start",
    "test": "jest",
    "test:snyk": "snyk test"
  },
  "dependencies": {
    "next": "^12.1.2",
    "react": "18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@joshuagardiner/typescript-component-library": "^1.0.0-alpha.1",
    "@netlify/plugin-nextjs": "^4.3.1",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^13.0.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/styled-components": "^5.1.24",
    "classnames": "^2.3.1",
    "concurrently": "^7.0.0",
    "eslint": "^8.12.0",
    "eslint-config-next": "^12.1.2",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.6.1",
    "react-icons": "^4.3.1",
    "sass": "^1.49.9",
    "snyk": "^1.890.0",
    "spellchecker-cli": "^4.8.1",
    "styled-components": "^5.3.5",
    "typescript": "^4.6.3"
  }
}
