{
  "name": "jg-dev",
  "private": true,
  "version": "1.2.0",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "fix:prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,scss,json,md,mdx}\"",
    "lint": "concurrently npm:lint:*",
    "lint:next": "next lint",
    "lint:prettier": "prettier --check \"src/**/*.{js,jsx,ts,tsx,scss,json,md,mdx}\"",
    "lint:spellcheck": "spellchecker -f *.md ./**/*.md -d .spelling -l en-GB",
    "prepare": "husky install",
    "start": "next start",
    "test": "jest",
    "test:snyk": "snyk test"
  },
  "dependencies": {
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@joshuagardiner/typescript-component-library": "^1.1.28",
    "@netlify/plugin-nextjs": "^4.2.7",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@types/jest": "^27.4.1",
    "@types/node": "17.0.21",
    "@types/react": "17.0.40",
    "@types/react-dom": "^17.0.13",
    "@types/styled-components": "^5.1.24",
    "classnames": "^2.3.1",
    "concurrently": "^7.0.0",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.6.0",
    "react-icons": "^4.3.1",
    "sass": "^1.49.9",
    "snyk": "^1.870.0",
    "spellchecker-cli": "^4.8.1",
    "styled-components": "^5.3.3",
    "typescript": "4.6.2"
  }
}
